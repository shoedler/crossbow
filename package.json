    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "esbuild-svelte": "^0.7.3",
    "jest": "^29.5.0",
    "obsidian": "latest",
    "svelte": "^3.59.1",
    "svelte-preprocess": "^5.0.4",
    "ts-jest": "^29.1.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
